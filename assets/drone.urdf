      <inertia ixx="0.000001" ixy="0" ixz="0" iyy="0.000001" iyz="0" izz="0.000001"/>
    </inertial>
  </link>

  <!-- Joints -->
  
  <!-- Front Left Arm Joint -->
  <joint name="front_left_arm_joint" type="fixed">
    <parent link="base_link"/>
    <child link="front_left_arm"/>
    <origin xyz="0.15 0.15 0" rpy="0 1.5708 0.7854"/>
  </joint>

  <!-- Front Right Arm Joint -->
  <joint name="front_right_arm_joint" type="fixed">
    <parent link="base_link"/>
    <child link="front_right_arm"/>
    <origin xyz="0.15 -0.15 0" rpy="0 1.5708 -0.7854"/>
  </joint>

  <!-- Rear Left Arm Joint -->
  <joint name="rear_left_arm_joint" type="fixed">
    <parent link="base_link"/>
    <child link="rear_left_arm"/>
    <origin xyz="-0.15 0.15 0" rpy="0 1.5708 2.3562"/>
  </joint>

  <!-- Rear Right Arm Joint -->
  <joint name="rear_right_arm_joint" type="fixed">
    <parent link="base_link"/>
    <child link="rear_right_arm"/>
    <origin xyz="-0.15 -0.15 0" rpy="0 1.5708 -2.3562"/>
  </joint>

  <!-- Front Left Propeller Joint -->
  <joint name="front_left_propeller_joint" type="continuous">
    <parent link="front_left_arm"/>
    <child link="front_left_propeller"/>
    <origin xyz="0.15 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <!-- Front Right Propeller Joint -->
  <joint name="front_right_propeller_joint" type="continuous">
    <parent link="front_right_arm"/>
    <child link="front_right_propeller"/>
